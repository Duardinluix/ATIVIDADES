#include <stdio.h>

void batalha (){
    int turnos = 5;
    
    for (int i = 1; i<= turnos; i++){
     printf ("turno %d\n:", i);
     printf("o espartano avanÃ§a!\n");
}
 printf ("espartano vence");
 return turnos;
}

int main() {
    
    int valor = 5;
    
    
    printf("Exercicio 1:\n ");
    batalha(valor);
    
   
    return 0;
}